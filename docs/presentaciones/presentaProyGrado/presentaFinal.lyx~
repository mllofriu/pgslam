\begin_layout BeginFrame
Métricas de rendimiento
\end_layout

\begin_layout Itemize
Eficiencia del recorrido
\end_layout

\begin_deeper
\begin_layout Itemize
Cociente entre el cubrimiento útil total y la distancia recorrida
\end_layout

\begin_deeper
\begin_layout Standard

\size small
\begin_inset Formula $\mathit{EficienciaRecorrido=CubrimientoUtilTotal/DistanciaRecorrida}$
\end_inset


\begin_inset Note Note
status open

\begin_layout Plain Layout

\size small
mejorar la indentacion
\end_layout

\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Itemize
Error medio en la estimación de la posición (RMS)
\end_layout

\begin_deeper
\begin_layout Itemize
Toma como referencia la posición obtenida con Doraemon
\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula $\mathit{RMS=\sqrt{\frac{1}{n}\sum_{1}^{n}(x_{estimada}-x_{Doraemon})^{2}}}$
\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Itemize
Ejecución exitosa o errónea
\end_layout

\begin_deeper
\begin_layout Itemize
Estudia la razón de terminación de las pruebas
\end_layout

\end_deeper
\begin_layout Itemize
Evolución del cubrimiento útil total
\end_layout

\begin_deeper
\begin_layout Itemize
Estudia la evolución de los sistemas con el paso del tiempo
\end_layout

\end_deeper
