package pc.cubrimiento;

public class Corrida {

	public int getCubrimientoUtil() {
		return cubrimientoUtil;
	}
	public int getCubrimientoInutil() {
		return cubrimientoInutil;
	}
	public float getDistRecorrida() {
		return distRecorrida;
	}
	private int cubrimientoUtil;
	private int cubrimientoInutil;
	private float distRecorrida;
	public Corrida(int cubrimientoUtil, int cubrimientoInutil,
			float distRecorrida) {
		super();
		this.cubrimientoUtil = cubrimientoUtil;
		this.cubrimientoInutil = cubrimientoInutil;
		this.distRecorrida = distRecorrida;
	}
}
